{
  "extends": "airbnb-base",
  "rules": {
    "spaced-comment": [0],
    "no-unused-vars": [0],
    "no-empty": [0],
    "no-constant-condition": [0],
    "arrow-body-style": [0],
    "semi": [0],
    "no-param-reassign": [0],
    "quotes": [0],
    "comma-dangle": [0],
    "no-console": [0],
    "class-methods-use-this": [0],
    "curly": [0],
    "no-class-assign": [0],
    "no-restricted-syntax": [0],
    "no-loop-func": [0],
    "no-use-before-define": [0],
    "func-names": [0]
  }
}
